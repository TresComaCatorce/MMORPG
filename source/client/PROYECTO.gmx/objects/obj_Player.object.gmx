        <arguments>
          <argument>
            <kind>1</kind>
            <string>//-----------------------------------------------------------------------------
// Evento custom que envia el update al servidor de los siguientes atributos:
// @header [String] Accion a informar, en este caso "POS_UPDATE".
// @posX [intS32] Nueva posicion X del personaje.
// @posY [intS32] Nueva posicion Y del personaje.
// @dir [intS32] Nueva direccion del personaje.
// @state [String] Nuevo 'state' del personaje.
//
// Se llama en:
//      -Al presionar la W para mover el personaje.
//-----------------------------------------------------------------------------

//Se crea el buffer a enviar
var pos_packet = buffer_create(1, buffer_grow, 1);

//Se escribe el header (accion para identificar en el servidor) en el buffer a enviar.
//Un string indicando la accion informada.
buffer_write(pos_packet, buffer_string, "POS_UPDATE");

//Se escriben los datos en el buffer a enviar.
//Dos enteros signed 32, uno para la posicion X y otro para la posicion Y.
buffer_write(pos_packet, buffer_s32, self.x);
buffer_write(pos_packet, buffer_s32, self.y);
//Un entero signed 32 para la direccion del personaje.
buffer_write(pos_packet, buffer_s32, self.dir);
//Un string para el 'state' del personaje (Para las animaciones).
buffer_write(pos_packet, buffer_s32, self.state);


//show_debug_message( "X: " + string(self.x) );
//show_debug_message( "Y: " + string(self.y) );


//Se escribe el socket creado en el "Network.socket" para que lo maneje y envie al servidor.
network_write(Network.socket, pos_packet);

</string>
          </argument>
        </arguments>
      </action>
    </event>
