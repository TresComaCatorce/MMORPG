        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Acción del boton

event_inherited();

if( mouse_check_button_pressed(mb_left) )
{
    //Si tiene hover se ejecuta la accion
    if( self.hover )
    {
        if( string_length(txt_Username.text) &gt; 0 
        &amp;&amp;  string_length( txt_Password.text ) &gt; 0 )
        {
            //Creo el paquete de login para enviar al servidor.
            var login_packet = buffer_create( 1, buffer_grow, 1 );
            
            //Escribo los valores en el paquete.
            buffer_write( login_packet, buffer_string, "C_LOGIN" );
            buffer_write( login_packet, buffer_string, txt_Username.text );
            buffer_write( login_packet, buffer_string, txt_Password.text );
            
            network_write( Network.socket, login_packet );
        }
        else
        {
            show_message( "Error: Username or password can't be empty." );
        }
    }
}
</string>
